---
import Layout from "@/layouts/Layout.astro";
import { build_ast } from "@/stglang/ASTBuilder";
import { sum_prg } from "@/stglang/test";
import { status, reason } from "@/tests/backtest";
let ast: any = "";
ast = build_ast(String(sum_prg));
---

<Layout title="List sum test.">
	<main>
		<div>
			<span>{status}</span>
			<span>{reason}</span>
		</div>
		<pre>{sum_prg}</pre>
		<pre>{ast}</pre>
	</main>
</Layout>
